{
	"name": "@tyrsolutions/moleculer-db-mikroorm-adapter",
	"version": "1.0.0",
	"description": "MikroORM adapter for moleculer",
	"main": "dist/index.js",
	"module": "dist/index.esm.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"installDeps": "pnpm install && cd ./docgen && pnpm install",
		"dev": "nodemon examples/index.js",
		"ci": "jest --watch",
		"clean": "pnpm clean:packages && pnpm install",
		"clean:packages": "rm -fr dist node_modules",
		"generateDocumentation": "node ./docgen/index.js -t ./templates/README.md -s ./dist/index.js -d README.md",
		"serveDocs": "docsify serve docs",
		"test": "jest --coverage",
		"lint": "eslint --ext=.js src test",
		"deps": "npm-check -u",
		"coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
		"postversion": "git push && git push --follow-tags",
		"pre-release": "pnpm build:clean && run-p build:dist",
		"build-release": "pnpm build:clean && set NODE_ENV=release&& run-p build:dist",
		"build:clean": "rimraf dist bin",
		"build:dist": "rollup -c",
		"build:dts": "dts-bundle-generator -o dist/index.d.ts src/index.ts",
		"release": "np"
	},
	"keywords": [
		"moleculer",
		"microservice",
		"mikro-orm"
	],
	"homepage": "https://tyrsolution.github.io/moleculer-db-mikroorm-adapter",
	"repository": {
		"type": "git",
		"url": "https://github.com/tyrsolution/moleculer-db-mikroorm-adapter.git"
	},
	"author": "Tyr Solutions",
	"license": "MIT",
	"devDependencies": {
		"@ourparentcenter/moleculer-decorators-extended": "^2.1.0",
		"@rollup/plugin-auto-install": "^3.0.5",
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-json": "^6.0.1",
		"@rollup/plugin-typescript": "^11.1.5",
		"@types/bluebird": "^3.5.42",
		"@types/core-js": "^2.5.8",
		"@types/flat": "^5.0.5",
		"@types/jest": "^29.5.10",
		"@types/lodash": "^4.14.202",
		"@types/node": "^20.10.0",
		"@typescript-eslint/eslint-plugin": "^6.12.0",
		"@typescript-eslint/parser": "^6.12.0",
		"benchmarkify": "^4.0.0",
		"coveralls": "^3.1.1",
		"docsify": "^4.13.1",
		"docsify-cli": "^4.4.4",
		"dts-bundle-generator": "^8.1.2",
		"esbuild": "^0.19.7",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-import": "^2.29.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-prettier": "^5.0.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-security": "^1.7.1",
		"jest": "^29.7.0",
		"jest-chain": "^1.1.6",
		"jest-circus": "^29.7.0",
		"jest-cli": "^29.7.0",
		"jest-extended": "^4.0.2",
		"jest-junit": "^16.0.0",
		"kleur": "^4.1.5",
		"moleculer-db": "^0.8.25",
		"nodemon": "^3.0.1",
		"np": "^8.0.4",
		"npm-check": "^6.0.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.1.0",
		"rimraf": "^5.0.5",
		"rollup": "^4.5.2",
		"rollup-plugin-auto-external": "^2.0.0",
		"rollup-plugin-dts-bundle": "^1.0.0",
		"rollup-plugin-esbuild": "^6.1.0",
		"rollup-plugin-node-builtins": "^2.1.2",
		"rollup-plugin-uglify": "^6.0.4",
		"ts-jest": "^29.1.1",
		"tsconfig-paths": "^4.2.0",
		"tslib": "^2.6.2",
		"typeof-items": "^1.0.1",
		"typescript": "^5.3.2"
	},
	"dependencies": {
		"@mikro-orm/better-sqlite": "^5.9.4",
		"@mikro-orm/core": "^5.9.4",
		"@mikro-orm/knex": "^5.9.4",
		"@mikro-orm/mariadb": "^5.9.4",
		"@mikro-orm/mongodb": "^5.9.4",
		"@mikro-orm/mysql": "^5.9.4",
		"@mikro-orm/postgresql": "^5.9.4",
		"@mikro-orm/sqlite": "^5.9.4",
		"bluebird": "^3.7.2",
		"flat": "^6.0.1",
		"lodash": "^4.17.21",
		"moleculer": "^0.14.32",
		"reflect-metadata": "^0.1.13"
	},
	"jest": {
		"coverageDirectory": "<rootDir>/coverage",
		"testEnvironment": "node",
		"collectCoverage": false,
		"coveragePathIgnorePatterns": [
			"<rootDir>/docgen"
		],
		"reporters": [
			"default",
			"jest-junit"
		],
		"transform": {
			"^.+\\.(ts|tsx)$": [
				"ts-jest",
				{
					"tsconfig": "tsconfig.json"
				}
			]
		},
		"setupFiles": [],
		"setupFilesAfterEnv": [
			"jest-chain",
			"jest-extended/all"
		],
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"engines": {
		"node": ">= 16.x.x"
	},
	"publishConfig": {
		"access": "public"
	}
}
